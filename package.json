{
  "name": "@curvenote/nbtx",
  "private": "true",
  "license": "MIT",
  "version": "0.0.1",
  "description": "Curvenote Notebook Tranlators",
  "author": "Steve Purves <steve@curvenote.com>",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "src": "src"
  },
  "files": [
    "src",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/curvenote/nbtx.git"
  },
  "scripts": {
    "test": "env FIRESTORE_EMULATOR_HOST=localhost:4000 NODE_ENV=test jest --config=jest.config.js --env=node --detectOpenHandles --forceExit --verbose",
    "test:dev": "env FIRESTORE_EMULATOR_HOST=localhost:4000 NODE_ENV=test jest --config=jest.config.js --env=node --detectOpenHandles",
    "test:watch": "env FIRESTORE_EMULATOR_HOST=localhost:4000 NODE_ENV=test jest --config=jest.config.js --watchAll --env=node --detectOpenHandles",
    "prepublishOnly": "npm run compile",
    "build": "tsc",
    "compile": "npm run clean && tsc -p .",
    "lint": "eslint \"src/**/*.ts\" -c ../../.eslintrc.js",
    "lint:format": "npx prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "lint:format:fix": "npx prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "watch": "tsc -w -p .",
    "clean": "rm -rf dist"
  },
  "bugs": {
    "url": "https://github.com/curvenote/curvenote/issues"
  },
  "dependencies": {},
  "peerDependencies": {
    "@curvenote/blocks": "1.3.0"
  },
  "devDependencies": {
    "@curvenote/blocks": "1.3.0",
    "@types/jest": "^26.0.23",
    "eslint": "6.8.0",
    "jest": "26.6.0",
    "ts-jest": "^26.5.6",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=12.0.0 <=^14.0.0"
  }
}
