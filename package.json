{
  "name": "mystjs",
  "version": "0.0.1",
  "description": "Markdown parser for MyST markdown in JavaScript",
  "author": "Rowan Cockett <rowanc1@gmail.com>",
  "homepage": "https://github.com/executablebooks/mystjs",
  "license": "MIT",
  "main": "dist/src/index.js",
  "unpkg": "dist/myst.min.js",
  "types": "dist/src/index.d.ts",
  "keywords": [
    "markdown-it-plugin",
    "markdown-it",
    "markdown",
    "myst",
    "jupyter",
    "jupyter-book"
  ],
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/executablebooks/mystjs.git"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "compile": "npm run clean && tsc -p .",
    "build": "tsc; webpack --config webpack.prod.js",
    "prepublishOnly": "npm run compile; npm run build;",
    "test": "node --experimental-vm-modules ./node_modules/.bin/jest",
    "test:watch": "node --experimental-vm-modules ./node_modules/.bin/jest --watchAll",
    "test:clear": "node --experimental-vm-modules ./node_modules/.bin/jest --clearCache",
    "start": "webpack serve --config webpack.dev.js",
    "dev": "webpack --config webpack.dev.js",
    "lint": "eslint \"src/**/*.ts\" -c .eslintrc.json --max-warnings 1",
    "lint:format": "prettier --write src/*.ts src/**/*.ts",
    "lint:format:fix": "eslint \"src/**/*.ts\" -c .eslintrc.json --fix",
    "watch": "tsc -w -p ."
  },
  "bugs": {
    "url": "https://github.com/executablebooks/mystjs/issues"
  },
  "dependencies": {
    "markdown-it": "^12.3.2",
    "markdown-it-amsmath": "^0.2.0",
    "markdown-it-docutils": "file:.yalc/markdown-it-docutils",
    "markdown-it-dollarmath": "^0.3.0",
    "rehype-stringify": "^9.0.3",
    "remark-rehype": "^10.1.0",
    "unified": "^10.1.1",
    "unist-util-map": "^3.0.0",
    "unist-util-visit": "^4.1.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.16.7",
    "@sinonjs/fake-timers": "^9.1.0",
    "@types/jest": "^27.4.0",
    "@types/js-yaml": "^4.0.5",
    "@types/jsdom": "^16.2.14",
    "@types/markdown-it": "^12.2.3",
    "@types/mdast": "^3.0.10",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "copy-webpack-plugin": "^10.2.4",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "eslint-plugin-no-skip-tests": "^1.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "express": "^4.17.2",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.5.1",
    "js-yaml": "^4.1.0",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.5",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "webpack-merge": "^5.8.0"
  }
}
