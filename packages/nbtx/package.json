{
  "name": "nbtx",
  "license": "MIT",
  "version": "0.1.11",
  "description": "Notebook Tranlators",
  "author": "Steve Purves <steve@curvenote.com>",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "src": "src"
  },
  "files": [
    "src",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/curvenote/curvenote.git"
  },
  "scripts": {
    "test": "jest --config=jest.config.js --env=node --detectOpenHandles --forceExit --verbose",
    "test:dev": "jest --config=jest.config.js --env=node --detectOpenHandles",
    "test:watch": "jest --config=jest.config.js --watchAll --env=node --detectOpenHandles",
    "prepublishOnly": "npm run compile",
    "build": "npm run clean && tsc -p .",
    "build:watch": "tsc -w -p .",
    "lint": "eslint \"src/**/*.ts\" -c ./.eslintrc.js",
    "lint:format": "npx prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint:format:fix": "npx prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "clean": "rm -rf dist"
  },
  "bugs": {
    "url": "https://github.com/curvenote/curvenote/issues"
  },
  "dependencies": {
    "@jupyterlab/nbformat": "^3.3.2",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "uuid": "^8.3.2"
  },
  "peerDependencies": {
    "@curvenote/blocks": "^1.3.3"
  },
  "devDependencies": {
    "@curvenote/blocks": "^1.3.4",
    "@types/jest": "^28.1.6",
    "@types/uuid": "^8.3.4",
    "eslint": "^8.21.0",
    "eslint-config-curvenote": "*",
    "jest": "28.1.3",
    "prettier": "latest",
    "ts-jest": "^28.0.7",
    "typescript": "latest"
  }
}
